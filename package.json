{
  "name": "react-template",
  "version": "1.0.0",
  "description": "react template with webpack2 nodejs express ",
  "main": "index.js",
  "author": "BENEKA AZZEDDINE",
  "license": "MIT",
  "scripts": {
    "dev": "rimraf ./dist && ./node_modules/.bin/webpack-dev-server --inline --hot",
    "start:dev": "rimraf ./dist && babel-node dev_server.js",
    "build": "rimraf ./dist && webpack -p --config webpack.config.prod.js",
    "start:prod": "yarn run build && babel-node server.js",
    "start": "echo 'you need run: \n1- start:dev for development  \n2- start:prod for production   '",
    "test": "mocha"
  },
  "devDependencies": {
    "babel-core": "^6.23.1",
    "babel-loader": "^6.3.2",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-2": "^6.22.0",
    "chai": "^3.5.0",
    "html-webpack-plugin": "^2.28.0",
    "mocha": "^3.2.0",
    "react-hot-loader": "^1.3.1",
    "redux-devtools-extension": "^2.13.0",
    "redux-logger": "^2.8.2",
    "webpack": "^2.2.1",
    "webpack-dev-middleware": "^1.10.1",
    "webpack-dev-server": "^2.4.1",
    "webpack-hot-middleware": "^2.17.1",
    "write-file-webpack-plugin": "^3.4.2"
  },
  "dependencies": {
    "express": "^4.15.0",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.3",
    "redux": "^3.6.0"
  }
}
